#include <stdio.h>
#include <math.h>

//Variable declaration
    int i= 30, sum=0, s;
    //While loop
int main() {
    while(i>=1){
        s = pow(i,2);
        printf("%d\n", s);
        i--;
		sum = sum + i;
       
    }
    printf("Sum is: %d", sum);
    
    return 0;
}
