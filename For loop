#include <stdio.h>
#include <math.h>
//Variable declaration
int main() { 
    int i, sum=0, s, start, stop;
//Inputs
printf("Enter when to start countdown:");
scanf("%d", &start);
printf("Enter when to stop countdown:");
scanf("%d", &stop);
//for loop
 
    for(i=start;i>=stop;i--){
        s = pow(i,2);
        printf("%d\n", s);
        sum = sum + i;
       
    }
    printf("Sum is: %d", sum);
    
    return 0;
}
